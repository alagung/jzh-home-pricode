<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.raral.childdev" 
	android:versionCode="6" android:versionName="1.0.4">


    <application android:icon="@drawable/icon" android:label="@string/app_name" android:taskAffinity="@string/app_name">
        <activity   android:name="Launcher"
        			android:screenOrientation="landscape"
         			android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="TestMain" android:launchMode="singleInstance"></activity>
        <activity android:name="ChildDevMain" android:launchMode="singleInstance" android:screenOrientation="landscape"></activity>

    </application>
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
</manifest>